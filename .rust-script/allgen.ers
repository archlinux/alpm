#!/usr/bin/env rust-script

//! ```cargo
//! [dependencies]
//! pkg = { path = "PATH", package = "PKG", features = ["cli"] }
//! clap_allgen = "0.2.1"
//! testresult = "0.4.1"
//! ```

fn main() -> testresult::TestResult {
    let output_dir = &std::env::args().collect::<Vec<_>>()[1];

    clap_allgen::render_KIND::<pkg::cli::Cli>(&output_dir)?;

    Ok(())
}
